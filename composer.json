{
    "name": "asseco-voice/laravel-remote-relations",
    "description": "Laravel support for making relations between microservices",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "asseco-voice/laravel-common": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.3.1",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\RemoteRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\RemoteRelations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\RemoteRelations\\RemoteRelationsServiceProvider"
            ]
        }
    }
}
