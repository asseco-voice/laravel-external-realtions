{
  "name": "asseco-voice/laravel-remote-relations",
  "description": "Laravel support for making relations between microservices",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.3.1",
    "fzaninotto/faker": "^1.9.1"
  },
  "autoload": {
    "psr-4": {
      "Voice\\RemoteRelations\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Voice\\RemoteRelations\\RemoteRelationsServiceProvider"
      ]
    }
  }
}
